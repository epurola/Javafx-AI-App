<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="377.0" prefWidth="604.0" spacing="20.0" stylesheets="/styles.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.controllers.EmotionController">
    <children>
        <Label text="Emotion Detection" />
        <ImageView fx:id="currentFrame" fitHeight="347.0" fitWidth="661.0" pickOnBounds="true" preserveRatio="true" />
        <Button fx:id="button" mnemonicParsing="false" onAction="#startCamera" stylesheets="/styles.css" text="Start Camera" />
        <Button onAction="#switchToPrimary" stylesheets="/styles.css" text="Back" />
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
